<h1>Listing concerts</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Score</th>
    <th>Owner</th>
    <th>Band</th>
    <th>Lounge</th>
    <th>Booking</th>
    <th>Status</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @concerts.each do |concert| %>
  <tr>
    <td><%= concert.name %></td>
    <td><%= concert.score %></td>
    <td><%= concert.owner %></td>
    <td><%= concert.band %></td>
    <td><%= concert.lounge %></td>
    <td><%= concert.booking %></td>
    <td><%= concert.status %></td>
    <td><%= link_to 'Show', concert %></td>
    <td><%= link_to 'Edit', edit_concert_path(concert) %></td>
    <td><%= link_to 'Destroy', concert, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Concert', new_concert_path %>
